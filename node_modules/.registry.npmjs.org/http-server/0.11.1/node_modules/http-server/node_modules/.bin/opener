#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/opener/1.4.3/node_modules/opener/opener.js/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/opener/1.4.3/node_modules/opener/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/opener/1.4.3/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/opener/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules:/Users/SeanRuffolo/dev/node_modules:/Users/SeanRuffolo/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../opener/1.4.3/node_modules/opener/opener.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/opener/1.4.3/node_modules/opener/opener.js/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/opener/1.4.3/node_modules/opener/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/opener/1.4.3/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/opener/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules:/Users/SeanRuffolo/dev/node_modules:/Users/SeanRuffolo/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../opener/1.4.3/node_modules/opener/opener.js" "$@"
  ret=$?
fi
exit $ret

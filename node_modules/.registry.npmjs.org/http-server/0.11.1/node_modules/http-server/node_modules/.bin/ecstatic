#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/lib/ecstatic.js/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/lib/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules:/Users/SeanRuffolo/dev/node_modules:/Users/SeanRuffolo/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../ecstatic/3.3.0/node_modules/ecstatic/lib/ecstatic.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/lib/ecstatic.js/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/lib/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules/ecstatic/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/3.3.0/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/ecstatic/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/.registry.npmjs.org/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules:/Users/SeanRuffolo/dev/node_modules:/Users/SeanRuffolo/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../ecstatic/3.3.0/node_modules/ecstatic/lib/ecstatic.js" "$@"
  ret=$?
fi
exit $ret

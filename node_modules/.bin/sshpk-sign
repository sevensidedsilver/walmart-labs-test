#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/sshpk/bin/sshpk-sign/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/sshpk/bin/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/sshpk/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules:/Users/SeanRuffolo/dev/node_modules:/Users/SeanRuffolo/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../sshpk/bin/sshpk-sign" "$@"
  ret=$?
else 
  NODE_PATH="/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/sshpk/bin/sshpk-sign/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/sshpk/bin/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules/sshpk/node_modules:/Users/SeanRuffolo/dev/walmart-labs-test/node_modules:/Users/SeanRuffolo/dev/node_modules:/Users/SeanRuffolo/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../sshpk/bin/sshpk-sign" "$@"
  ret=$?
fi
exit $ret
